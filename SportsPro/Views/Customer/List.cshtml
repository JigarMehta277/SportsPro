@model List<SportsPro.Models.Customer>

@{
    ViewData["Title"] = "Customer List";
}

<h2>Customer List</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Postal Code</th>
            <th>Country ID</th>
            <th>Phone</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.FirstName</td>
                <td>@customer.LastName</td>
                <td>@customer.Address</td>
                <td>@customer.City</td>
                <td>@customer.State</td>
                <td>@customer.PostalCode</td>
                <td>@customer.CountryID</td>
                <td>@customer.Phone</td>
                <td>@customer.Email</td>
            </tr>
        }
    </tbody>
</table>
